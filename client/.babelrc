{
  "presets": [
    ["@babel/env", { "targets": "> 0.25%, not dead" }],
    "@babel/react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 2
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@atom": "./src/components/atom",
          "@styler": "@emotion/styled",
          "@root": "./src"
        }
      }
    ]
  ]
}
